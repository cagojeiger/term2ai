[run]
source = src/term2ai
parallel = false
data_file = .coverage
omit =
    */tests/*
    */__pycache__/*
    */.venv/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml
